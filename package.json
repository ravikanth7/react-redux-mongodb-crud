{
  "name": "resamobile",
  "version": "0.0.1",
  "description": "Styletag Isomorphic app using React and Redux",
  "scripts": {
    "test": "cross-env NODE_ENV=test PORT=8080 node_modules/.bin/nyc node --harmony-proxies node_modules/.bin/ava",
    "watch:test": "npm run test -- --watch",
    "cover": "nyc npm run test",
    "check-coverage": "nyc check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "start": "cross-env BABEL_DISABLE_CACHE=1 NODE_ENV=development nodemon index.js",
    "start:prod": "cross-env NODE_ENV=production node index.js",
    "bs": "npm run clean && npm run build && npm run build:server && npm run start:prod",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.prod.js",
    "build:server": "cross-env NODE_ENV=production webpack --config webpack.config.server.js",
    "clean": "rimraf dist",
    "slate": "rimraf node_modules && npm install",
    "lint": "eslint client server"
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/styletag/resamobile.git"
  },
  "bugs": {
    "url": "https://github.com/styletag/resamobile/issues"
  },
  "homepage": "https://github.com/styletag/resamobile#readme",
  "author": "Ravikanth",
  "license": "MIT",
  "dependencies": {
    "aws-sdk": "^2.22.0",
    "babel-core": "^6.21.0",
    "body-parser": "^1.17.0",
    "classnames": "^2.2.5",
    "cluster": "^0.7.7",
    "compression": "^1.6.2",
    "cookie-parser": "^1.4.3",
    "cross-env": "^3.1.4",
    "crypto-js": "^3.1.9-1",
    "express": "^4.15.0",
    "font-awesome": "^4.7.0",
    "immutable": "^3.8.1",
    "isomorphic-fetch": "^2.2.1",
    "jsonwebtoken": "^7.4.1",
    "limax": "^1.4.1",
    "log4js": "^1.1.1",
    "mongoose": "^4.11.1",
    "mime": "^1.3.4",
    "morgan": "^1.8.1",
    "newrelic": "^1.37.2",
    "prop-types": "^15.5.8",
    "qs": "^6.3.1",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-helmet": "^5.0.3",
    "react-redux": "^5.0.3",
    "react-router": "^3.0.2",
    "react-router-dom": "^4.1.1",
    "react-router-redux": "4.0.8",
    "react-router-scroll": "^0.4.2",
    "react-slick": "^0.14.11",
    "react-lazy-load": "^3.0.12",
    "redis": "^2.7.1",
    "redux": "^3.6.0",
    "redux-form": "^6.7.0",
    "redux-immutable": "^3.1.0",
    "redux-thunk": "^2.2.0",
    "request": "^2.79.0",
    "reselect": "3.0.0",
    "sanitize-html": "^1.14.1",
    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "ava": "^0.18.2",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-regenerator": "^6.24.1",
    "babel-plugin-webpack-loaders": "^0.9.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-es2015-native-modules": "^6.9.4",
    "babel-preset-react": "^6.23.0",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.23.0",
    "chai": "^3.5.0",
    "chunk-manifest-webpack-plugin": "1.0.0",
    "coveralls": "^2.11.16",
    "css-loader": "^0.28.1",
    "css-modules-require-hook": "^4.0.5",
    "cssnano": "^3.10.0",
    "enzyme": "^2.7.1",
    "eslint": "^3.16.1",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-ava": "^4.2.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.0",
    "eventsource-polyfill": "^0.9.6",
    "extract-text-webpack-plugin": "^2.0.0",
    "file-loader": "^0.10.1",
    "ignore-loader": "^0.1.2",
    "jsdom": "^9.11.0",
    "json-loader": "^0.5.4",
    "mock-css-modules": "^1.0.0",
    "nock": "^9.0.9",
    "nodemon": "^1.11.0",
    "null-loader": "^0.1.1",
    "nyc": "^10.1.2",
    "postcss-cssnext": "^2.9.0",
    "postcss-focus": "^1.0.0",
    "postcss-load-config": "^1.2.0",
    "postcss-loader": "^1.3.3",
    "postcss-reporter": "^3.0.0",
    "pre-commit": "^1.2.2",
    "react-addons-test-utils": "^15.4.2",
    "react-addons-perf": "^15.4.2",
    "react-hot-loader": "^3.0.0-beta.2",
    "redux-ava": "^2.2.0",
    "redux-devtools": "^3.3.2",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.2.0",
    "rimraf": "^2.6.1",
    "sinon": "^1.17.7",
    "style-loader": "^0.13.2",
    "supertest": "^3.0.0",
    "url-loader": "^0.5.8",
    "webpack": "2.5.1",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-dev-server": "^2.4.1",
    "webpack2-externals-plugin": "^1.0.0",
    "webpack-hot-middleware": "^2.17.1",
    "webpack-manifest-plugin": "^1.1.0"
  },
  "engines": {
    "node": ">=4"
  },
  "ava": {
    "files": [
      "client/**/*.spec.js",
      "server/**/*.spec.js"
    ],
    "source": [
      "client/**/*.js",
      "server/**/*.js"
    ],
    "failFast": true,
    "babel": "inherit",
    "require": [
      "./server/util/setup-test-env.js"
    ]
  },
  "nyc": {
    "include": [
      "client/**/*.js",
      "server/**/*.js"
    ],
    "exclude": [
      "**/*.spec.js",
      "client/reducers.js",
      "client/store.js",
      "client/routes.js",
      "server/util/setup-test-env.js"
    ],
    "reporter": [
      "lcov",
      "text",
      "html"
    ]
  }
}
